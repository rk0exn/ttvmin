<Application x:Class="ttvmin.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:helpers="clr-namespace:ttvmin.Helpers"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<helpers:ColorConverter x:Key="ColorConverter"/>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Controls/ButtonNx.xaml"/>
				<ResourceDictionary Source="/Controls/DarkStyles.xaml"/>
				<ResourceDictionary Source="/Controls/LightStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
